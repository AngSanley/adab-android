<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ambinusian.adab">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/adab_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Adab">
        <activity android:name=".Setting.SettingActivity"></activity>
        <activity android:name=".Help.HelpActivity" />
        <activity android:name=".Calendar.CalendarActivity" />
        <activity android:name=".Forum.ForumActivity" />
        <activity android:name=".Topics.TopicsActivity" />
        <activity android:name=".MainActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>