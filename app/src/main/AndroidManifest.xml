<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ambinusian.adab">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ADAB to Connect"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Adab">
        <activity android:name=".ui.lecturer.coursedetail.CourseDetail"></activity>
        <activity android:name=".ui.lecturer.main.MainActivity" />
        <activity android:name=".ui.student.livesession.LiveSessionActivity" />
        <activity
            android:name=".ui.splash.SplashActivity"
            android:theme="@style/Theme.Adab.Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.student.main.MainActivity" />
    </application>

</manifest>